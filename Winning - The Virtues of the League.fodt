   <text:p text:style-name="P32"><text:span text:style-name="T19">Winning: </text:span><text:span text:style-name="T20">Virtues of the League</text:span></text:p>
   <text:p text:style-name="P33">A Free Tactics and Strategy Guide for League of Legends</text:p>
   <text:p text:style-name="P19"/>
   <text:p text:style-name="P21">Version 0.1</text:p>
   <text:p text:style-name="P19"/>
   <text:p text:style-name="P19"/>
   <text:p text:style-name="P19"/>
